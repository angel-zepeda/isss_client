{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "import/prefer-default-export": "off",
        "arrow-parens": 1,
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "no-await-in-loop": "off",
        "no-console": [
            "error",
            {
                "allow": "all"
            }
        ]
    }
}